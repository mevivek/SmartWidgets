    <style name="AppTheme" parent="Theme.MaterialComponents.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorSecondary">@color/colorAccent</item>
        <item name="colorOnPrimary">@color/colorOnPrimary</item>
        <item name="android:textColor">@color/textColorSecondary</item>
        <item name="android:textColorPrimary">@color/textColorSecondary</item>
        <item name="android:textColorSecondary">@color/textColorSecondary</item>
        <item name="colorSurface">@color/colorSurface</item>
        <item name="colorOnSurface">@color/textColorSecondary</item>
        <item name="colorControlNormal">@color/textColorSecondary</item>
        <item name="android:fontFamily">@font/gotham_book</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="alertDialogTheme">@style/materialAlertDialogTheme</item>
        <item name="android:windowBackground">@color/colorSurface</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.Launcher">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@drawable/launch_screen</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="foodLabelColorSwitch">
        <item name="colorControlActivated">@android:color/holo_green_light</item>
        <item name="colorSwitchThumbNormal">@android:color/holo_red_light</item>
        <item name="android:colorForeground">@android:color/holo_red_dark</item>
    </style>
    <style name="materialAlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="android:background">@color/colorSurface</item>
    </style>
    <declare-styleable name="Circular">
        <attr format="enum" name="shape">
            <enum name="square" value="1"/>
            <enum name="rectangle" value="2"/>
            <enum name="circle" value="3"/>
        </attr>
        <attr format="string" name="imageUrl"/>
        <attr format="reference" name="imagePlaceholder"/>
        <attr format="integer" name="widthRatio"/>
        <attr format="integer" name="heightRation"/>
        <attr format="color" name="strokeColor"/>
        <attr format="dimension" name="strokeWidth"/>
        <attr format="boolean" name="highlightEnabled"/>
        <attr format="color" name="highlightColor"/>
        <attr format="string" name="text"/>
        <attr format="color" name="textColor"/>
        <attr format="dimension" name="textSize"/>
        <attr format="color" name="avatarBackgroundColor"/>
        <attr format="enum" name="mode">
            <enum name="initial" value="1"/>
            <enum name="image" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="SmartImageView">
        <attr format="enum" name="ratio">
            <enum name="square_by_width" value="1"/>
            <enum name="square_by_height" value="2"/>
        </attr>

        <attr format="string" name="url"/>
        <attr format="reference" name="placeholder"/>
    </declare-styleable>
